    <command:details>
      <command:name>New-OSCSPSiteGroup</command:name>
      <maml:description>
        <maml:para>Creates a new SharePoint group.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>New</command:verb>
      <command:noun>OSCSPSiteGroup</command:noun>
      <dev:version></dev:version>
    </command:details>
    <maml:description>
      <maml:para>The New-OSCSPSiteGroup creates a new SharePoint group and sets its member and permissions. </maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>New-OSCSPSiteGroup</maml:name>
        <command:parameter globbing="false" variableLength="false" position="1" required="true" pipelineInput="false">
          <maml:name>SiteUrl</maml:name>
          <maml:description>
            <maml:para>Specifies the URL of a SharePoint site, in the form http://server_name/sites/sitename.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="2" required="true" pipelineInput="false">
          <maml:name>Name</maml:name>
          <maml:description>
            <maml:para>Specifies the name of a SharePoint group that will be created.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="3" required="true" pipelineInput="false">
          <maml:name>Description</maml:name>
          <maml:description>
            <maml:para>Specifies the description of this SharePoint group.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="4" required="true" pipelineInput="false">
          <maml:name>Owner</maml:name>
          <maml:description>
            <maml:para>Specifies the owner of this SharePoint group, in the form domain\user.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="5" required="false" pipelineInput="false">
          <maml:name>Member</maml:name>
          <maml:description>
            <maml:para>Specifies the group members that will be added to this SharePoint group.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="6" required="false" pipelineInput="false">
          <maml:name>PermissionLevel</maml:name>
          <maml:description>
            <maml:para>Specifies the permission level that you want members of this SharePoint group to have on this site.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
