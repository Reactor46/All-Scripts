<?xml version="1.0" encoding="utf-16"?>

<!-- #### please replace "MySQLServerInstanceName" by your SQL Server instance name #### -->

<Influx-Capacitor>

  <CounterGroups>
	<CounterGroup Name="perfmon.ssas.cache.current_in_kilobytes" SecondsInterval="20">
		<Counter>
			<CategoryName>MSOLAP$MySQLServerInstanceName:Cache</CategoryName>
			<CounterName>Current KB</CounterName>
			<InstanceName></InstanceName>
		</Counter>
	</CounterGroup>
