<?xml version="1.0" encoding="UTF-8" ?>
<SpaceSnifferConfig>
    <refreshDelayMs>50</refreshDelayMs>
    <zoomTimeMs>5</zoomTimeMs>
    <minElementSizePx>10</minElementSizePx>
    <showFreeSpace>false</showFreeSpace>
    <showUnknownSpace>false</showUnknownSpace>
    <initialStyle>flat</initialStyle>
    <showFileTooltipDateTime>true</showFileTooltipDateTime>
