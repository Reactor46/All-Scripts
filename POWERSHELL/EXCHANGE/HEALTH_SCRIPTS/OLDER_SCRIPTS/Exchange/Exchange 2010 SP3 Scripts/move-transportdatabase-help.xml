          <maml:para>C:\PS&gt;</maml:para>
        </maml:introduction>
        <dev:code>Move-TransportDatabase -TemporaryStoragePath d:\Exchange\Temp</dev:code>
        <dev:remarks />
      </command:example>
      <command:example>
        <maml:title _locID="Example_Title_2">-------------------------- EXAMPLE 2 --------------------------</maml:title>
